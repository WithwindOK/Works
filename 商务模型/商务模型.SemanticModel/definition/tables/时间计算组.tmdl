table 时间计算组
	lineageTag: cb16019d-307f-435a-ba2e-3c3749059864

	calculationGroup
		precedence: 20

		calculationItem 季累 =
				
				VAR _today=TODAY()
				VAR _mindate=MAX('D_日历'[日期])
				RETURN
				    IF(
				        _mindate<=_today,
				        SWITCH(
				            TRUE(),
				            CONTAINSSTRING(SELECTEDMEASURENAME(),"M."),CALCULATE(SELECTEDMEASURE(), DATESQTD('D_日历'[日期])),
				            SELECTEDMEASURE()
				        )
				    )

		calculationItem 年累 =
				
				VAR _today=TODAY()
				VAR _mindate=MIN('D_日历'[日期])
				RETURN
				    IF(
				        _mindate<=_today,
				        SWITCH(
				            TRUE(),
				            CONTAINSSTRING(SELECTEDMEASURENAME(),"M."),CALCULATE(SELECTEDMEASURE(), DATESYTD('D_日历'[日期])),
				            SELECTEDMEASURE()
				        )
				    )

		calculationItem 开累 =
				
				VAR _today=TODAY()
				VAR _mindate=MIN('D_日历'[日期])
				RETURN
				    IF(
				        _mindate<=_today,
				        SWITCH(
				            TRUE(),
				            CONTAINSSTRING(SELECTEDMEASURENAME(),"M."),CALCULATE( SELECTEDMEASURE() , 'D_日历'[日期]<=MAX('D_日历'[日期]) ),
				            SELECTEDMEASURE()
				        )
				    )

		calculationItem 基值 = SELECTEDMEASURE()

